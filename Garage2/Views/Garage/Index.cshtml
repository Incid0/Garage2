@model IEnumerable<Garage2.Vehicle>
@{
    ViewBag.Title = "Överblick parkerade fordon";
}
@section scripts{
<script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
}
<p>
</p>
@using (Ajax.BeginForm(
                    new AjaxOptions
                    {
                        HttpMethod = "get",
                        InsertionMode = InsertionMode.Replace,
                        UpdateTargetId = "vehicleList"
                    }))
{
    <label for="vehiclefilter">Filtrera: </label>
    <input id="vehiclefilter" type="search" name="filter" />
    <input type="submit" name="filtersubmit" />
    <p></p>
    @Html.Partial("_Vehicles", Model)
}
<p>
    <a href="@Url.Action("Park")" class="btn btn-primary btn-lg">Parkera fordon</a>
</p>
